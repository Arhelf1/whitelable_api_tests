{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "title",
    "documents"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "53009107"
      ],
      "pattern": "^(.*)$"
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "Дистанционный единый счет_WhiteLabel 1"
      ],
      "pattern": "^(.*)$"
    },
    "documents": {
      "$id": "#/properties/documents",
      "type": "array",
      "title": "The Documents Schema",
      "items": {
        "$id": "#/properties/documents/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "parentId",
          "title",
          "created",
          "canSign",
          "canCancel",
          "canAddFile",
          "isSigned",
          "description",
          "documentFiles",
          "docTypeId"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/documents/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              53009108
            ]
          },
          "parentId": {
            "$id": "#/properties/documents/items/properties/parentId",
            "type": "null",
            "title": "The Parentid Schema",
            "default": null,
            "examples": [
              null
            ]
          },
          "title": {
            "$id": "#/properties/documents/items/properties/title",
            "type": "string",
            "title": "The Title Schema",
            "default": "",
            "examples": [
              "Заявление о получении доступа к Transaq Connector_ЭДО"
            ],
            "pattern": "^(.*)$"
          },
          "created": {
            "$id": "#/properties/documents/items/properties/created",
            "type": "string",
            "title": "The Created Schema",
            "default": "",
            "examples": [
              "2019-07-08T10:51:15.217"
            ],
            "pattern": "^(.*)$"
          },
          "canSign": {
            "$id": "#/properties/documents/items/properties/canSign",
            "type": "boolean",
            "title": "The Cansign Schema",
            "default": false,
            "examples": [
              false
            ]
          },
          "canCancel": {
            "$id": "#/properties/documents/items/properties/canCancel",
            "type": "boolean",
            "title": "The Cancancel Schema",
            "default": false,
            "examples": [
              false
            ]
          },
          "canAddFile": {
            "$id": "#/properties/documents/items/properties/canAddFile",
            "type": "boolean",
            "title": "The Canaddfile Schema",
            "default": false,
            "examples": [
              false
            ]
          },
          "isSigned": {
            "$id": "#/properties/documents/items/properties/isSigned",
            "type": "boolean",
            "title": "The Issigned Schema",
            "default": false,
            "examples": [
              true
            ]
          },
          "description": {
            "$id": "#/properties/documents/items/properties/description",
            "type": "null",
            "title": "The Description Schema",
            "default": null,
            "examples": [
              null
            ]
          },
          "documentFiles": {
            "$id": "#/properties/documents/items/properties/documentFiles",
            "type": "array",
            "title": "The Documentfiles Schema",
            "items": {
              "$id": "#/properties/documents/items/properties/documentFiles/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "id",
                "documentId",
                "name",
                "canSign"
              ],
              "properties": {
                "id": {
                  "$id": "#/properties/documents/items/properties/documentFiles/items/properties/id",
                  "type": "integer",
                  "title": "The Id Schema",
                  "default": 0,
                  "examples": [
                    93909921
                  ]
                },
                "documentId": {
                  "$id": "#/properties/documents/items/properties/documentFiles/items/properties/documentId",
                  "type": "integer",
                  "title": "The Documentid Schema",
                  "default": 0,
                  "examples": [
                    53009108
                  ]
                },
                "name": {
                  "$id": "#/properties/documents/items/properties/documentFiles/items/properties/name",
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "Заявление о получении доступа к Transaq Connector_ЭДО.pdf"
                  ],
                  "pattern": "^(.*)$"
                },
                "canSign": {
                  "$id": "#/properties/documents/items/properties/documentFiles/items/properties/canSign",
                  "type": "boolean",
                  "title": "The Cansign Schema",
                  "default": false,
                  "examples": [
                    false
                  ]
                }
              }
            }
          },
          "docTypeId": {
            "$id": "#/properties/documents/items/properties/docTypeId",
            "type": "integer",
            "title": "The Doctypeid Schema",
            "default": 0,
            "examples": [
              2406
            ]
          }
        }
      }
    }
  }
}